{
  "name": "@noise-xyz/noise-types",
  "version": "1.0.10",
  "description": "Type definitions for Noise project",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./database/*": {
      "types": "./dist/database/*.d.ts",
      "default": "./dist/database/*.js"
    },
    "./database/relations": {
      "types": "./dist/database/relations/index.d.ts",
      "default": "./dist/database/relations/index.js"
    },
    "./database/relations/*": {
      "types": "./dist/database/relations/*.d.ts",
      "default": "./dist/database/relations/*.js"
    },
    "./database/tables": {
      "types": "./dist/database/tables/index.d.ts",
      "default": "./dist/database/tables/index.js"
    },
    "./database/tables/*": {
      "types": "./dist/database/tables/*.d.ts",
      "default": "./dist/database/tables/*.js"
    },
    "./database/types": {
      "types": "./dist/database/types/index.d.ts",
      "default": "./dist/database/types/index.js"
    },
    "./database/types/*": {
      "types": "./dist/database/types/*.d.ts",
      "default": "./dist/database/types/*.js"
    },
    "./trading/*": {
      "types": "./dist/trading/*.d.ts",
      "default": "./dist/trading/*.js"
    },
    "./events/*": {
      "types": "./dist/events/*.d.ts",
      "default": "./dist/events/*.js"
    },
    "./logs/*": {
      "types": "./dist/logs/*.d.ts",
      "default": "./dist/logs/*.js"
    },
    "./positions/*": {
      "types": "./dist/positions/*.d.ts",
      "default": "./dist/positions/*.js"
    },
    "./conversions/*": {
      "types": "./dist/conversions/*.d.ts",
      "default": "./dist/conversions/*.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "default": "./dist/utils/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "database/*": [
        "./dist/database/*"
      ],
      "database/relations": [
        "./dist/database/relations"
      ],
      "database/relations/*": [
        "./dist/database/relations/*"
      ],
      "database/tables": [
        "./dist/database/tables"
      ],
      "database/tables/*": [
        "./dist/database/tables/*"
      ],
      "database/types": [
        "./dist/database/types"
      ],
      "database/types/*": [
        "./dist/database/types/*"
      ],
      "trading/*": [
        "./dist/trading/*"
      ],
      "events/*": [
        "./dist/events/*"
      ],
      "logs/*": [
        "./dist/logs/*"
      ],
      "positions/*": [
        "./dist/positions/*"
      ],
      "conversions/*": [
        "./dist/conversions/*"
      ],
      "utils/*": [
        "./dist/utils/*"
      ]
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "bun build/generateExports.ts",
    "build": "tsc",
    "prepare": "bun run build",
    "prepublish": "bun run build",
    "version": "git add -A",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "drizzle-orm": "^0.36.0"
  }
}
