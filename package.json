{
    "name": "@noise-xyz/noise-types",
    "version": "1.0.5",
    "description": "Type definitions for Noise",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "private": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*/index.d.ts",
            "default": "./dist/*/index.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*"
            ]
        }
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "prepare": "npm run build",
        "prepublish": "npm run build",
        "version": "git add -A",
        "postversion": "git push && git push --tags"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com",
        "access": "restricted"
    },
    "type": "module",
    "devDependencies": {
        "@types/bun": "latest",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "bignumber.js": "^9.1.2",
        "drizzle-orm": "^0.36.0"
    }
}
